	<queryString>
		<![CDATA[SELECT
     concat('JO',LPAD(ABS(jo.id),8,'0')) AS refId,
     a.`description` AS description,
     truncate(od.`amount`,2) as amount
FROM
	 `official_receipt` orcpt,
     `job_orders` jo,
     `job_order_type` jot,
     `amenity` a,
     `amenities_request` ar,
     `official_receipt_details` od,
     `official_receipt_header` oh
WHERE
	 orcpt.or_number = $P{p_or_number}
     and oh.or_number = orcpt.or_number
     and od.header_id = oh.id
     and orcpt.job_order_id = od.ref_num
     and orcpt.job_order_id = jo.id
     and jo.order_type_id = jot.id
     and jo.request_id = ar.id
     and a.id = ar.amenity_id
     and jot.id = '1'
UNION
SELECT
     concat('JO',LPAD(ABS(jo.id),8,'0')) AS refId,
     s.`description` AS description,
     truncate(od.`amount`,2) as amount
FROM
     `official_receipt` orcpt,
     `job_orders` jo,
     `job_order_type` jot,
     `service` s,
     `service_request` sr,
     `official_receipt_details` od,
     `official_receipt_header` oh
WHERE
     orcpt.or_number = $P{p_or_number}
     and oh.or_number = orcpt.or_number
     and od.header_id = oh.id
     and orcpt.job_order_id = od.ref_num
     and orcpt.job_order_id = jo.id
     and jo.order_type_id = jot.id
     and jo.request_id = sr.id
     and s.id = sr.service_id
     and jot.id = '2'
UNION
SELECT
     concat('JO',LPAD(ABS(jo.id),8,'0')) AS refId,
     wr.remarks AS description,
     truncate(od.`amount`,2) as amount
FROM
     `official_receipt` orcpt,
     `job_orders` jo,
     `job_order_type` jot,
     `water_reading` wr,
     `official_receipt_details` od,
     `official_receipt_header` oh
WHERE
     orcpt.or_number = $P{p_or_number}
     and oh.or_number = orcpt.or_number
     and od.header_id = oh.id
     and orcpt.job_order_id = od.ref_num
     and orcpt.job_order_id = jo.id
     and jo.order_type_id = jot.id
     and jo.request_id = wr.id
     and jot.id = '3'
UNION
SELECT
     concat('JO',LPAD(ABS(jo.id),8,'0')) AS refId,
     ad.remarks AS description,
     truncate(od.`amount`,2) as amount
FROM
     `official_receipt` orcpt,
     `job_orders` jo,
     `job_order_type` jot,
     `association_due` ad,
     `official_receipt_details` od,
     `official_receipt_header` oh
WHERE
     orcpt.or_number = $P{p_or_number}
     and oh.or_number = orcpt.or_number
     and od.header_id = oh.id
     and orcpt.job_order_id = od.ref_num
     and orcpt.job_order_id = jo.id
     and jo.order_type_id = jot.id
     and jo.request_id = ad.id
     and jot.id = '4'
UNION
SELECT
     concat('JO',LPAD(ABS(jo.id),8,'0')) AS refId,
     adj.remarks AS description,
     truncate(od.`amount`,2) as amount
FROM
     `official_receipt` orcpt,
     `job_orders` jo,
     `job_order_type` jot,
     `adjustment` adj,
     `official_receipt_details` od,
     `official_receipt_header` oh
WHERE
     orcpt.or_number = $P{p_or_number}
     and oh.or_number = orcpt.or_number
     and od.header_id = oh.id
     and orcpt.job_order_id = od.ref_num
     and orcpt.job_order_id = jo.id
     and jo.order_type_id = jot.id
     and jo.request_id = adj.id
     and jot.id = '6']]>
	</queryString>
